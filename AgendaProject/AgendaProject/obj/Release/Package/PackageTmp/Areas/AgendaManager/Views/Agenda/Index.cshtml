@model IEnumerable<AgendaProject.Models.Agenda>

@{
    ViewBag.Title = "Index";
}

<h2>Preliminary Agendas Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MeetingDateTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AgendaType)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.MeetingDateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AgendaType)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.Id })
            </td>
        </tr>
    }

</table>
